{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "^antd(.*)",
    "^styled(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/",
    "^(?!.*style$)[./]",
    ".*/style"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
